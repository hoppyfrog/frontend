{
  "name": "frontend",
  "version": "1.0.0",
  "description": "React frontend for barapp",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "mocha --compilers js:babel-register --recursive",
    "posttest": "npm run lint",
    "lint": "eslint ./",
    "build": "webpack",
    "deploy": "npm run test && npm run build && surge --project dist/ --domain barmatcher.surge.sh",
    "schema-fetch": "reindex schema-fetch",
    "schema-push": "reindex schema-push",
    "schema-relay": "reindex schema-relay relay/schema.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoppyfrog/frontend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hoppyfrog/frontend/issues"
  },
  "homepage": "https://github.com/hoppyfrog/frontend#readme",
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-relay": "^0.7.3",
    "reindex-js": "^0.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-relay-plugin-loader": "^0.7.3",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "reindex-cli": "^0.3.4",
    "surge": "^0.17.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "metadata": {
    "graphql": {
      "schema": "./relay/schema.json"
    }
  }
}
